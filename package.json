		"@types/node": "^20.14.13",
		"@typescript-eslint/eslint-plugin": "^6.18.0",
		"@typescript-eslint/parser": "^6.18.0",
		"eslint": "^8.56.0",
		"prisma": "^5.19.0",
		"typescript": "^5.3.3"
	},
	"engines": {
		"node": ">=20.2.0"
	},
	"dependencies": {
		"@prisma/client": "^5.19.0"
	}
}